<?xml version="1.0" ?>
<robot name="cygnus" xmlns:xacro="http://ros.org/wiki/xacro" >

  <xacro:property name="namespace" value="cygnus" />
  <xacro:property name="motor_constant" value="8.54858e-06" /> <!-- [kg.m/s^2] -->
  <xacro:property name="moment_constant" value="0.016" /> <!-- [m] -->
  <xacro:property name="time_constant_up" value="0.0125" /> <!-- [s] -->
  <xacro:property name="time_constant_down" value="0.025" /> <!-- [s] -->
  <xacro:property name="max_rot_velocity" value="838" /> <!-- [rad/s] -->
  <xacro:property name="rotor_drag_coefficient" value="8.06428e-05" />
  <xacro:property name="rolling_moment_coefficient" value="0.000001" />
  <xacro:property name="rotor_velocity_slowdown_sim" value="10" />

  <gazebo>
      <static>false</static>
  </gazebo>

  <link name="base_link"></link>
  <joint name="random_joint" type="fixed">
    <parent link="base_link"/>
    <child link="cygnus_cuerpo"/>
    <origin rpy="0  0  0" xyz="0 0 0"/>
    <axis xyz="0  0  0"/>
  </joint>


  <link name="cygnus_cuerpo">
    <inertial>
      <mass value="1.3"/>
      <origin rpy="0  0  0" xyz="0   0   0.01"/>
      <inertia ixx="0.0052239" ixy="0.00018965" ixz="0.00005" iyy="0.0053164" iyz="-0.00004588" izz="0.0089147"/>
    </inertial>
    <collision>
      <origin rpy="0  0  0" xyz="0   0   0.01"/>
      <geometry>
        <box size="0.76 0.76 0.15"/>
        <!-- [m] [m] [m] -->
      </geometry>
    </collision>
    <visual >
      <origin rpy="0  0  0" xyz="0   0   0.01"/>
      <geometry>
        <mesh filename="package://cygnus_description/meshes/cuerpo.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>



  <link name="cygnus_aspa_1">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0  0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual >
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa1.dae" scale="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>



  <link name="cygnus_aspa_2">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0   0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual >
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa1.dae" scale="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>

  <link name="cygnus_aspa_3">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual >
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa3_4.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>


  <link name="cygnus_aspa_4">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa3_4.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>


<joint name="cygnus_revo_motor_1" type="continuous">
    <parent link="base_link"/>
    <child link="cygnus_aspa_1"/>
    <origin rpy="0  0  0" xyz="0.224 0 0.062"/>
    <axis xyz="0  0  1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>


  <joint name="cygnus_revo_motor_3" type="continuous">
    <parent link="base_link"/>
    <child link="cygnus_aspa_3"/>
    <origin rpy="0  0  0" xyz="-0.224 0 0.062"/>
    <axis xyz="0  0  1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>


  <joint name="cygnus_revo_motor_2" type="continuous">
    <parent link="base_link"/>
    <child link="cygnus_aspa_2"/>
    <origin rpy="0  0  0" xyz="0.0 -0.224 0.062"/>
    <axis xyz="0  0 -1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>


  <joint name="cygnus_revo_motor_4" type="continuous">
    <parent link="base_link"/>
    <child link="cygnus_aspa_4"/>
    <origin rpy="0  0  0" xyz="0.0 0.224 0.062"/>
    <axis xyz="0  0 -1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>


<gazebo >
  <!--name="motor_constant" value="8.54858e-06" /--> <!-- [kg.m/s^2] -->
  <!--name="moment_constant" value="0.016" /--> <!-- [m] -->
  <!--name="time_constant_up" value="0.0125" /--> <!-- [s] -->
  <!--name="time_constant_down" value="0.025" /--> <!-- [s] -->
  <!--name="max_rot_velocity" value="838" /--> <!-- [rad/s] -->
  <!--name="rotor_drag_coefficient" value="8.06428e-05" /-->
  <!--name="rolling_moment_coefficient" value="0.000001" /--> 
      <plugin name="front_right_motor_model" filename="librotors_gazebo_motor_model.so">
        <robotNamespace>${namespace}</robotNamespace>
        <jointName>cygnus_revo_motor_1</jointName>
        <linkName>cygnus_aspa_1</linkName>
        <turningDirection>ccw</turningDirection>
        <timeConstantUp>${time_constant_up}</timeConstantUp>
        <timeConstantDown>${time_constant_down}</timeConstantDown>
        <maxRotVelocity>${max_rot_velocity}</maxRotVelocity>
        <motorConstant>${motor_constant}</motorConstant>
        <momentConstant>${moment_constant}</momentConstant>
        <commandSubTopic>gazebo/command/motor_speed</commandSubTopic>
        <motorNumber>1</motorNumber>
        <rotorDragCoefficient>${rotor_drag_coefficient}</rotorDragCoefficient>
        <rollingMomentCoefficient>${rolling_moment_coefficient}</rollingMomentCoefficient>
        <motorSpeedPubTopic>motor_speed/1</motorSpeedPubTopic>
        <rotorVelocitySlowdownSim>${rotor_velocity_slowdown_sim}</rotorVelocitySlowdownSim>
      </plugin>
    </gazebo>


<gazebo >
      <plugin name="back_left_motor_model" filename="librotors_gazebo_motor_model.so">
        <robotNamespace>${namespace}</robotNamespace>
        <jointName>cygnus_revo_motor_3</jointName>
        <linkName>cygnus_aspa_3</linkName>
        <turningDirection>ccw</turningDirection>
        <timeConstantUp>${time_constant_up}</timeConstantUp>
        <timeConstantDown>${time_constant_down}</timeConstantDown>
        <maxRotVelocity>${max_rot_velocity}</maxRotVelocity>
        <motorConstant>${motor_constant}</motorConstant>
        <momentConstant>${moment_constant}</momentConstant>
        <commandSubTopic>gazebo/command/motor_speed</commandSubTopic>
        <motorNumber>3</motorNumber>
        <rotorDragCoefficient>${rotor_drag_coefficient}</rotorDragCoefficient>
        <rollingMomentCoefficient>${rolling_moment_coefficient}</rollingMomentCoefficient>
        <motorSpeedPubTopic>motor_speed/3</motorSpeedPubTopic>
        <rotorVelocitySlowdownSim>${rotor_velocity_slowdown_sim}</rotorVelocitySlowdownSim>
      </plugin>
    </gazebo>

<gazebo >
      <plugin name="front_left_motor_model" filename="librotors_gazebo_motor_model.so">
        <robotNamespace>${namespace}</robotNamespace>
        <jointName>cygnus_revo_motor_2</jointName>
        <linkName>cygnus_aspa_2</linkName>
        <turningDirection>cw</turningDirection>
        <timeConstantUp>${time_constant_up}</timeConstantUp>
        <timeConstantDown>${time_constant_down}</timeConstantDown>
        <maxRotVelocity>${max_rot_velocity}</maxRotVelocity>
        <motorConstant>${motor_constant}</motorConstant>
        <momentConstant>${moment_constant}</momentConstant>
        <commandSubTopic>gazebo/command/motor_speed</commandSubTopic>
        <motorNumber>2</motorNumber>
        <rotorDragCoefficient>${rotor_drag_coefficient}</rotorDragCoefficient>
        <rollingMomentCoefficient>${rolling_moment_coefficient}</rollingMomentCoefficient>
        <motorSpeedPubTopic>motor_speed/2</motorSpeedPubTopic>
        <rotorVelocitySlowdownSim>${rotor_velocity_slowdown_sim}</rotorVelocitySlowdownSim>
      </plugin>
    </gazebo>

<gazebo>
      <plugin name="back_right_motor_model" filename="
      librotors_gazebo_motor_model.so">
        <robotNamespace>${namespace}</robotNamespace>
        <jointName>cygnus_revo_motor_4</jointName>
        <linkName>cygnus_aspa_4</linkName>
        <turningDirection>cw</turningDirection>
        <timeConstantUp>${time_constant_up}</timeConstantUp>
        <timeConstantDown>${time_constant_down}</timeConstantDown>
        <maxRotVelocity>${max_rot_velocity}</maxRotVelocity>
        <motorConstant>${motor_constant}</motorConstant>
        <momentConstant>${moment_constant}</momentConstant>
        <commandSubTopic>gazebo/command/motor_speed</commandSubTopic>
        <motorNumber>4</motorNumber>
        <rotorDragCoefficient>${rotor_drag_coefficient}</rotorDragCoefficient>
        <rollingMomentCoefficient>${rolling_moment_coefficient}</rollingMomentCoefficient>
        <motorSpeedPubTopic>motor_speed/4</motorSpeedPubTopic>
        <rotorVelocitySlowdownSim>${rotor_velocity_slowdown_sim}</rotorVelocitySlowdownSim>
      </plugin>
    </gazebo>

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find cygnus_description)/model/cygnus.gazebo" />

</robot>
