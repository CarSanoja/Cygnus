<?xml version="1.0" ?>
<robot name="Cygnus" xmlns:xacro="http://www.ros.org/wiki/xacro" >

  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find cygnus_description)/model/cygnus.gazebo" />
  
  <gazebo>
      <static>false</static>
  </gazebo>

  <link name="base_link"></link>
  <joint name="random_joint" type="fixed">
    <parent link="base_link"/>
    <child link="Cygnus_Cuerpo"/>
    <origin rpy="0  0  0" xyz="0 0 0"/>
    <axis xyz="0  0  0"/>
  </joint>


  <link name="Cygnus_Cuerpo">
    <inertial>
      <mass value="1.3"/>
      <origin rpy="0  0  0" xyz="0   0   0.01"/>
      <inertia ixx="0.0052239" ixy="0.00018965" ixz="0.00005" iyy="0.0053164" iyz="-0.00004588" izz="0.0089147"/>
    </inertial>
    <collision>
      <origin rpy="0  0  0" xyz="0   0   0.01"/>
      <geometry>
        <box size="0.76 0.76 0.15"/>
        <!-- [m] [m] [m] -->
      </geometry>
    </collision>
    <visual >
      <origin rpy="0  0  0" xyz="0   0   0.01"/>
      <geometry>
        <mesh filename="package://cygnus_description/meshes/cuerpo.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>



  <link name="Cygnus__Aspa_1">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0  0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual >
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa1_2.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>



  <link name="Cygnus__Aspa_2">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0   0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual >
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa1_2.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>

  <link name="Cygnus__Aspa_3">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual >
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa3_4.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>


  <link name="Cygnus__Aspa_4">
    <inertial>
      <mass value="0.007"/>
      <origin rpy="0  0  0" xyz="0 0 0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0.0000041" iyy="0.000019" iyz="0" izz="0.000001"/>
    </inertial>
    <collision >
      <geometry>
        <cylinder length="0.006" radius="0.1"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <mesh filename="package://cygnus_description/meshes/aspa3_4.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>


<joint name="Cygnus_revo_motor1" type="continuous">
    <parent link="base_link"/>
    <child link="Cygnus__Aspa_1"/>
    <origin rpy="0  0  0" xyz="0.224 0 0.1"/>
    <axis xyz="0  0  1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>

  <joint name="Cygnus_revo_motor2" type="continuous">
    <parent link="base_link"/>
    <child link="Cygnus__Aspa_2"/>
    <origin rpy="0  0  0" xyz="-0.224 0 0.1"/>
    <axis xyz="0  0  1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>

  <joint name="Cygnus_revo_motor3" type="continuous">
    <parent link="base_link"/>
    <child link="Cygnus__Aspa_3"/>
    <origin rpy="0  0  0" xyz="0.0 0.224 0.062"/>
    <axis xyz="0  0 -1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>

  <joint name="Cygnus_revo_motor4" type="continuous">
    <parent link="base_link"/>
    <child link="Cygnus__Aspa_4"/>
    <origin rpy="0  0  0" xyz="0.0 -0.224 0.062"/>
    <axis xyz="0  0 -1"/>
    <limit effort="0.01428" velocity="254"/>
  </joint>



</robot>
